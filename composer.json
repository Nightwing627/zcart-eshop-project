{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ahkmunna/paypalpayment": "dev-master",
        "algolia/algoliasearch-client-php": "^1.27",
        "artesaos/seotools": "^0.14.0",
        "authorizenet/authorizenet": "^1.9",
        "aws/aws-sdk-php": "^3.80",
        "consoletvs/charts": "6.*",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.8",
        "guzzlehttp/guzzle": "^6.5",
        "incevio/cybersource-wrapper": "^1.0",
        "instamojo/instamojo-php": "^0.3.0",
        "ixudra/curl": "^6.16",
        "laracasts/flash": "^3.0",
        "laravel/cashier": "^9.0",
        "laravel/framework": "5.7.*",
        "laravel/scout": "6.1.1",
        "laravel/socialite": "^4.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.7",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "league/glide-laravel": "^1.0",
        "nao-pon/flysystem-google-drive": "^1.1",
        "ramsey/uuid": "^3.8",
        "rap2hpoutre/fast-excel": "^0.11.0",
        "setasign/fpdf": "^1.8",
        "spatie/laravel-activitylog": "^3.1",
        "spatie/laravel-analytics": "^3.6",
        "spatie/laravel-backup": "^5.11",
        "spatie/laravel-cors": "^1.5",
        "spatie/laravel-image-optimizer": "^1.3",
        "spatie/laravel-newsletter": "^4.2",
        "stripe/stripe-php": "^6.24",
        "symfony/polyfill-iconv": "^1.10",
        "teamtnt/laravel-scout-tntsearch-driver": "^6.1",
        "torann/geoip": "^1.0",
        "tucker-eric/eloquentfilter": "^1.3",
        "yab/laravel-scout-mysql-driver": "^2.1",
        "yabacon/paystack-php": "^2.1",
        "yajra/laravel-datatables-oracle": "^8.13"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "deployer/recipes": "^6.1",
        "filp/whoops": "^2.0",
        "laravel/dusk": "^4.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpspec/phpspec": "^5.1",
        "phpunit/phpunit": "^7.0",
        "symfony/css-selector": "^4.2",
        "symfony/dom-crawler": "^4.2",
        "tanmuhittin/laravel-google-translate": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
             "type": "composer",
             "url": "https://packagist.org"
        },
        { "packagist": false }
    ]
}
